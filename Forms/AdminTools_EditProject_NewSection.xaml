<?xml version="1.0" encoding="utf-8"?>
<Window x:Class="FivesBronxTimesheetManagement.Forms.AdminTools_EditProject_NewSection" Title="AdminTools_EditProject_NewSection" SizeToContent="WidthAndHeight" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Grid>
    <StackPanel Orientation="Horizontal">
      <StackPanel MinWidth="100" Margin="5">
        <Label Content="Project" />
        <Label Content="Is Warranty" />
        <Label Content="Section" />
        <Label Content="Section Description" />
        <Label Content="Task Type" />
        <Label Content="Activity Number" />
        <Button Margin="5" Content="Cancel" IsCancel="True" />
      </StackPanel>
      <StackPanel MinWidth="150" Margin="5">
        <Label Name="lblProject" Content="" Height="26" />
        <CheckBox Name="chbxWarranty" IsChecked="False" Height="26" />
        <TextBox Name="txtSection" Height="26" />
        <TextBox Name="txtSectionDescription" Height="26" />
        <ComboBox Name="cbxTaskType" Height="26" />
        <TextBox Name="txtActivity" Height="26" />
        <Button Name="btnOk" Margin="5" Content="OK" Click="btnOk_Click" />
      </StackPanel>
    </StackPanel>
  </Grid>
</Window>